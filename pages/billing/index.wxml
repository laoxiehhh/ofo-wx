<!--pages/billing/index.wxml-->
<view class='container'>
  <view class='number'>
    <text>当前单车编号： {{username}}</text>
  </view>
  <view class='time'>
    <view class='time-title'>
      {{actionText}}
    </view>
    <view class='time-content'>
      {{hours}}: {{minutes}}: {{seconds}}
    </view>
  </view>
  <view class='endride'>
    <button type='warn' bindtap='endride' disabled='{{clickkey}}'>结束骑行</button>
    <button type='primary' bindtap='movetomap'>回到地图</button>
  </view>
</view>
