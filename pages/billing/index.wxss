/* pages/billing/index.wxss */
.container {
  width: 100%;
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center; 
   /* justify-content: space-between;  */
}
.number, .endride {
   flex-grow: 2; 
  width: 100%;
  text-align: center;
  padding: 60rpx 0;
}
.time {
   flex-grow: 6; 
  width: 100%;
  text-align: center;
}
.time .time-content {
  font-size: 100rpx;
}
.endride button {
  width: 90%;
  margin-top: 40rpx;
}
